	<navigation-rule>
		<display-name>postagem</display-name>
		<from-view-id>/criarPostagem_admin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logoff</from-outcome>
			<to-view-id>/home.xhtml</to-view-id>
