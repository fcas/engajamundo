<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">

	<managed-bean>
		<managed-bean-name>controllerPostagem</managed-bean-name>
		<managed-bean-class>controller.ControllerPostagem</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>controllerSession</managed-bean-name>
		<managed-bean-class>controller.ControllerSession</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<display-name>singup</display-name>
		<from-view-id>/singup.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/operacaoSucesso.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/operacaoFail.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>Edicao</display-name>
		<from-view-id>/edicao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/operacaoSucesso.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/operacaoFail.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logoff</from-outcome>
			<to-view-id>/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>autenticacao</display-name>
		<from-view-id>/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>engajador</from-outcome>
			<to-view-id>/home_engajador.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/home_admin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>buscas</display-name>
